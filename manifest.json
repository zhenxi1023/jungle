{
  "manifest_version": 2,
  "name": "Jungle",
  "version": "0.0.1",
  "description": "amz",
  "browser_action": {
    "default_popup": "popup.html",
    "permissions": ["background"]
  },
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
    "http://*/*",
    "https://*/*",
    "tabs",
    "activeTab"
  ],
  "content_script": [{
    "matches": ["https://www.github.com"],
    "js": ["main.js"]
  }, {
    "matches": ["https://www.github.com"],
    "js": ["operate.js"]
  }],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}